   server {       #yo les potes
	server_name test;
	listen 10.33.3.5:80;

	root html/site1/;
	index monsite.html;

	max_body_size 10000000;

	cgi /usr/bin/php-cgi,/usr/bin/python3,/usr/bin/bash;
	cgi_ext .php,.py,.sh;

	allow_methods GET,POST,DELETE;

	location /print.php {
		allow_methods GET,POST;
		root html/site1/;

		redirection /post.html;
	}

	location /lucas.php {
		allow_methods GET,POST;
		root html/site1/;

		redirection /post.html;
	}
}

