server {
	server_name test.com;
	listen 127.0.0.1:8082;

	root html/site1/;
	allow_methods GET, POST;
	autoindex on;

	location / {
		root www/ ;
		rewrite /blabla www/exemple.txt rewrite;
	}

	location /favicon. {
		root www/testhtml/favicon.;
	}

	location /www {
		root www/ ;
		autoindex off;
	}

	location /testhtml {
		root www/testhtml/;
	}

	location /cgi-bin {
		root cgi-bin/;
		cgi_ext .py;
	}
}

