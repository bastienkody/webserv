	server {
	server_name test;
	listen 10.33.3.5:80;

	root html/site1/;

	location / {
		index html/index.html;
		autoindex on;
		max_body_size 100000000;
		redirection /redir.html;
		error_page 404 /404.html;
		error_page 405 /405.html;
		allow_methods GET, POST, DELETE;
		cgi /usr/bin/php-cgi,/usr/bin/python3,/usr/bin/bash;
		cgi_ext .php,.py,.sh;
	}
}
